{
  "name": "react-md-components",
  "version": "1.2.3-b",
  "description": "This is a react module containing a free collection of material design components.",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack",
    "prettier": "prettier ./src/*/*.js --write",
    "lint:check": "eslint ./src/*/*.js",
    "lint:fix": "eslint ./src/*/*.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xNicklaj/react-md-components"
  },
  "keywords": [
    "Material",
    "Design",
    "MD",
    "React",
    "Google",
    "Material",
    "Material.IO"
  ],
  "author": "Simone Aronica",
  "license": "ISC",
  "dependencies": {
    "babel-jest": "^24.9.0",
    "eslint-plugin-react": "^7.17.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-jss": "^10.0.0",
    "react-test-renderer": "^16.12.0"
  },
  "files": [
    "dist",
    "src",
    "LICENSE"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "url-loader": "^3.0.0",
    "eslint": "^7.22.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverage": true
  }
}
